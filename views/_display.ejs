<div class="form-group row">
  <label class="col-form-label col-sm-2" for="username">Username</label>
  <div class="col-sm-8">
    <input readonly class="form-control" id="username" value="<%= model.username %>" />
  </div>
</div>

<div class="form-group row mt-2">
  <label class="col-form-label col-sm-2" for="password">Password</label>
  <div class="col-sm-7">
    <input readonly class="form-control" id="password" value="<%= model.password %>" />
  </div>
</div>

<div class="form-group row mt-2">
  <label class="col-form-label col-sm-2" for="email">Email</label>
  <div class="col-sm-10">
    <input readonly class="form-control" maxlength="3200" cols="20" id="email" value="<%= model.email %>"/>

  </div>
</div>

<div class="form-group row mt-2 mb-2">
  <label class="col-form-label col-sm-2" for="tel">Phone Number</label>
  <div class="col-sm-10">
    <input readonly class="form-control" type="tel" cols="20" id="tel" maxlength="10" value="<%= model.tel %>" />
  </div>
</div>