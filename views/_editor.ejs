<style>
    .a {
        color: black;
    }
</style>
<div class="form-group row">
    <label class="col-form-label col-sm-2 a" for="username">Username</label>
    <div class="col-sm-8">
        <input autofocus class="form-control" name="username" value="<%= model.username %>" />
    </div>
</div>

<div class="form-group row mt-2">
    <label class="col-form-label col-sm-2 a" for="password">Password</label>
    <div class="col-sm-7">
        <input class="form-control" name="password" value="<%= model.password %>" />
    </div>
</div>

<div class="form-group row mt-2">
    <label class="col-form-label col-sm-2 a" for="email">Email</label>
    <div class="col-sm-10">
        <input class="form-control" name="email" cols="20" maxlength="3200" value="<%= model.email %>" />
    </div>
</div>

<div class="form-group row mt-2 mb-2" a>
    <label class="col-form-label col-sm-2" for="tel">Phone Number</label>
    <div class="col-sm-7">
        <input class="form-control" name="tel" type="tel" maxlength="10" cols="20" value="<%= model.tel %>" />
    </div>
</div>